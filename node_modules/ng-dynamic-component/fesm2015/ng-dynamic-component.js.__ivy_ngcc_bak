    return (_c = (_b = (_a = ((/** @type {?} */ (type)))) === null || _a === void 0 ? void 0 : _a.ctorParameters) === null || _b === void 0 ? void 0 : _b.call(_a)) === null || _c === void 0 ? void 0 : _c.map((/**
     * @param {?} param
     * @return {?}
     */
    param => param.type));
}

