/**
 * @param {?} val
 * @return {?}
 */
function createNewChange(val) {
    return new SimpleChange(undefined, val, true);
}
/**
 * @param {?} val
 * @param {?} prevVal
 * @return {?}
 */
function createChange(val, prevVal) {
    return new SimpleChange(prevVal, val, false);
}
